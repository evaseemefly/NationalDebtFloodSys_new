FROM mini_py39_x64:1.14

ENV PYTHONUNBUFFERED 1

# 1- 根目录下创建 /code 目录
RUN mkdir -p /my_codes

# 2- 将 /code 设置为工作目录
WORKDIR /my_codes

# 3- 将本地目录下的文件全部拷贝至容器 /code 中
# v100
# COPY /public/home/surge/TYPHOON_PROJ/nation_debt_flood/servers/microserver_tyspider /my_codes
# pc
COPY /home/wam/my_proj/microserver_tyspider /my_codes
